package org.arr;


//wap(write a program) to copy one array to other array
public class Copy {

	public static void main(String[] args) {
		
		int k[] = {22, 4, -1, 5};
		
		int a[] = k;
		
		for(int i=0 ; i<k.length ; i++) {
			if(k[i]%2==0) {
				System.out.println("k["+i+"] = "+k[i]);
			}
		}
	}
}
