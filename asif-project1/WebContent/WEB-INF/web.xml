<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>asif-project1</display-name>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	
	
	<servlet>
		<servlet-name>userServlet</servlet-name>
		<servlet-class>org.ecom.controller.UserController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>userServlet</servlet-name>
		<url-pattern>/user/list</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
	<servlet-name>employeeServlet</servlet-name>
	<servlet-class>org.ecom.controller.EmployeeController</servlet-class>
	</servlet>
					
	<servlet-mapping>
	<servlet-name>employeeServlet</servlet-name>
	<url-pattern>/employeeController/list</url-pattern>
	</servlet-mapping>
					
	
	
	
	
	
	



	<servlet>
		<servlet-name>deleteServlet</servlet-name>
		<servlet-class>org.ecom.controller.UserDeleteController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>deleteServlet</servlet-name>
		<url-pattern>/user/delete</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>employeeDeleteServlet</servlet-name>
		<servlet-class>org.ecom.controller.EmployeeDeleteController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>employeeDeleteServlet</servlet-name>
		<url-pattern>/employeeDeleteController/delete</url-pattern>
	</servlet-mapping>
	
	
	
	





	
	<servlet>
		<servlet-name>createServlet</servlet-name>
		<servlet-class>org.ecom.controller.UserCreateController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>createServlet</servlet-name>
		<url-pattern>/user/add</url-pattern>
	</servlet-mapping>
	
				
	<servlet>
		<servlet-name>employeeCreateServlet</servlet-name>
		<servlet-class>org.ecom.controller.EmployeeCreateController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>employeeCreateServlet</servlet-name>
		<url-pattern>/employeeCreateController/add</url-pattern>
	</servlet-mapping>
	
	
	
	
	




	
	<servlet>
		<servlet-name>updateServlet</servlet-name>
		<servlet-class>org.ecom.controller.UserUpdateController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>updateServlet</servlet-name>
		<url-pattern>/user/update</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>employeeUpdateServlet</servlet-name>
		<servlet-class>org.ecom.controller.EmployeeUpdateController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>employeeUpdateServlet</servlet-name>
		<url-pattern>/employeeUpdateController/update</url-pattern>
	</servlet-mapping>
	
	
	
	





		
	<servlet>
		<servlet-name>detailServlet</servlet-name>
		<servlet-class>org.ecom.controller.UserDetailController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>detailServlet</servlet-name>
		<url-pattern>/user/detail</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>employeeDetailServlet</servlet-name>
		<servlet-class>org.ecom.controller.EmployeeDetailController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>employeeDetailServlet</servlet-name>
		<url-pattern>/employeeDetailController/detail</url-pattern>
	</servlet-mapping>
	
	
		
</web-app>